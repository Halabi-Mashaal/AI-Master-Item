# Environment Configuration for Render Deployment
USE_LIGHTWEIGHT_NLP=1
MEMORY_OPTIMIZED=1
DISABLE_HEAVY_MODELS=1

# AI Model API Keys (Add your API keys here)
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Choose AI Provider: 'openai', 'gemini', or 'fallback' (default)
AI_PROVIDER=openai

# Optional Render-specific settings
WEB_CONCURRENCY=1
MAX_WORKERS=1
